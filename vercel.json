{
  "buildCommand": "",
  "outputDirectory": ".",
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/executive-summary",
      "destination": "/cohort-executive-summary.html"
    },
    {
      "source": "/agent-architecture",
      "destination": "/cohort-agent-architecture.html"
    },
    {
      "source": "/system-design",
      "destination": "/cohort-system-design.html"
    },
    {
      "source": "/ClaudesBrain",
      "destination": "/ClaudeBrain/index.html"
    },
    {
      "source": "/ClaudesBrain/overview",
      "destination": "/ClaudeBrain/claude-brain-overview.html"
    },
    {
      "source": "/ClaudesBrain/agents",
      "destination": "/ClaudeBrain/claude-brain-agents.html"
    },
    {
      "source": "/ClaudesBrain/commands",
      "destination": "/ClaudeBrain/claude-brain-commands.html"
    },
    {
      "source": "/ClaudesBrain/skills",
      "destination": "/ClaudeBrain/claude-brain-skills.html"
    },
    {
      "source": "/ClaudesBrain/hooks",
      "destination": "/ClaudeBrain/claude-brain-hooks.html"
    },
    {
      "source": "/ClaudesBrain/workflows",
      "destination": "/ClaudeBrain/claude-brain-workflows.html"
    }
  ]
}
